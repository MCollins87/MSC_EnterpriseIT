@article{westland2026,
  title = {Building {{Lovemarks}} with Social Media Marketing: {{A}} Methodological Comparison},
  shorttitle = {Building {{Lovemarks}} with Social Media Marketing},
  author = {Westland, J. Christopher},
  year = 2026,
  month = jan,
  journal = {Journal of Retailing and Consumer Services},
  volume = {88},
  pages = {104503},
  issn = {0969-6989},
  doi = {10.1016/j.jretconser.2025.104503},
  urldate = {2025-11-16},
  abstract = {This study provides a methodological exploration of the `Lovemarks' framework in social media marketing by replicating and extending the work of (Fetais et al., 2023a). While the original study found strong mediating effects using Partial Least Squares SEM (PLS-SEM) on a narrow sample, its conclusions may be sensitive to methodological choices. To investigate this, we conduct a large-scale simulation study (N~=~11,398) and analyze the data using both the original limited-information method (PLS-SEM) and a full-information method (Lavaan SEM-ML). The results reveal a stark contrast: while PLS-SEM supports the original findings, the more stringent full-information SEM challenges them, finding minimal or non-significant relationships. This study contributes by demonstrating the critical impact of methodological choice in social media research and cautions against over-reliance on variance-based SEM for complex theoretical models. It offers practical implications for both marketers and academic researchers by highlighting the divergent findings and underscoring the need for methodological rigor when examining the link between social media activities and brand loyalty.},
  keywords = {Brand loyalty,Community engagement,Lovemark theory,Luxury fashion brands,Social media marketing (SMM)},
  file = {/home/mark/snap/zotero-snap/common/Zotero/storage/KQFENJZD/Westland - 2026 - Building Lovemarks with social media marketing A methodological comparison.pdf;/home/mark/snap/zotero-snap/common/Zotero/storage/SMK4K9U3/S0969698925002826.html}
}

@article{yenipazarli2021,
  title = {The Marketplace Dilemma: {{Selling}} to the Marketplace vs. Selling on the Marketplace},
  shorttitle = {The Marketplace Dilemma},
  author = {Yenipazarli, Arda},
  year = 2021,
  journal = {Naval Research Logistics (NRL)},
  volume = {68},
  number = {6},
  pages = {761--778},
  issn = {1520-6750},
  doi = {10.1002/nav.21964},
  urldate = {2025-12-03},
  abstract = {Online marketplaces are rapidly shifting the trajectory of the e-commerce landscape. Brand manufacturers are starkly more dependent on online marketplaces, and for most brands, marketplace presence is not optional but mandatory. At this point, brand executives face a pivotal question in addressing their marketplace presence: What product selling (or distribution) approach should be adopted to leverage this channel for their brand? At its core, there are effectively two options offered to brand executives: selling to the online marketplace (as a 1p vendor); and selling on the marketplace platform (as a 3p seller). Whereas the conventional 1p vendor model is quintessentially the entry point to an online marketplace, some brand manufacturers migrate to the 3p seller model. Many others, however, avoid pulling the 3p trigger. In this paper, we address these two options a brand manufacturer has for selling (or allowing sales of) his product on a marketplace. On the grounds that online marketplaces retain brands at distinct price/quality tiers so as to be both comprehensive and robust, we propose a model of competition between two brand manufacturers whose products (in a category) are vertically differentiated on a quality/performance attribute and a convex marginal production cost is incurred for providing the higher quality. Given there is no single selling strategy on a marketplace that is ideal for all brand manufacturers and strategies would work the best under different market and competitive conditions, we investigate the impact of where a brand stands (vis-\'a-vis his competitor) on the two dimensions of a product-attribute space on the transitions of 1p brands to 3p sellers on the marketplace platform. We also extend the analysis to the setting where an online retailer decides on whether or not to add a marketplace platform to her existing online marketplace and (if so) on the referral fee percentage at which the product category would be listed.},
  langid = {english},
  keywords = {online marketplace,product positioning,upstream competition,vertical differentiation},
  file = {/home/mark/snap/zotero-snap/common/Zotero/storage/WGDED7IH/Yenipazarli - 2021 - The marketplace dilemma Selling to the marketplace vs. selling on the marketplace.pdf;/home/mark/snap/zotero-snap/common/Zotero/storage/RED3QNAL/nav.html}
}

